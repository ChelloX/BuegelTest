<?xml version="1.0" encoding="UTF-8" ?>
<configuration xmlns="metrics.config.woped.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<variables>
		<variable>
			<ID>N</ID>
			<name>Metrics.Variable.Name.N</name>
			<method>calculateN</method>
			<description>Metrics.Variable.Descr.N</description>
		</variable>
		<variable>
			<ID>T</ID>
			<name>Metrics.Variable.Name.T</name>
			<method>calculateT</method>
			<description>Metrics.Variable.Descr.T</description>
		</variable>
		<variable>
			<ID>P</ID>
			<name>Metrics.Variable.Name.P</name>
			<method>calculateP</method>
			<description>Metrics.Variable.Descr.P</description>
		</variable>
		<variable>
			<ID>A</ID>
			<name>Metrics.Variable.Name.A</name>
			<method>calculateA</method>
			<description>Metrics.Variable.Descr.A</description>
		</variable>
		<variable>
			<ID>SeqN</ID>
			<method>calculateSeqN</method>
			<description>Metrics.Variable.Descr.SeqN</description>
		</variable>
		<variable>
			<ID>SeqT</ID>
			<method>calculateSeqT</method>
			<description>Metrics.Variable.Descr.SeqT</description>
		</variable>
		<variable>
			<ID>SeqP</ID>
			<method>calculateSeqP</method>
			<description>Metrics.Variable.Descr.SeqP</description>
		</variable>
		<variable>
			<ID>Dia</ID>
			<name>Metrics.Variable.Name.Dia</name>
			<method>calculateDia</method>
			<description>Metrics.Variable.Descr.Dia</description>
		</variable>
		<variable>
			<ID>Cut</ID>
			<name>Metrics.Variable.Name.Cut</name>
			<method>calculateCut</method>
			<description>Metrics.Variable.Descr.Cut</description>
		</variable>
		<variable>
			<ID>RN</ID>
			<formula>N - SeqN</formula>
			<description>Metrics.Variable.Descr.RN</description>
		</variable>
		<variable>
			<ID>RT</ID>
			<formula>T - SeqT</formula>
			<description>Metrics.Variable.Descr.RT</description>
		</variable>
		<variable>
			<ID>RP</ID>
			<formula>P - SeqP</formula>
			<description>Metrics.Variable.Descr.RP</description>
		</variable>
		<variable>
			<ID>CycN</ID>
			<method>calculateCycN</method>
			<description>Metrics.Variable.Descr.CycN</description>
		</variable>
		<variable>
			<ID>TM</ID>
			<method>calculateTM</method>
			<description>Metrics.Variable.Descr.TM</description>
		</variable>
		<variable>
			<ID>TP</ID>
			<method>calculateTP</method>
			<description>Metrics.Variable.Descr.TP</description>
		</variable>
		<variable>
			<ID>PT</ID>
			<method>calculatePT</method>
			<description>Metrics.Variable.Descr.PT</description>
		</variable>
		<variable>
			<ID>TS</ID>
			<method>calculateTS</method>
			<description>Metrics.Variable.Descr.TS</description>
		</variable>
		<variable>
			<ID>TreeComp</ID>
			<method>calculateTreeComp</method>
			<description>Metrics.Variable.Descr.TreeComp</description>
		</variable>
		<variable>
			<ID>TrivialComp</ID>
			<method>calculateTrivialComp</method>
			<description>Metrics.Variable.Descr.TrivialComp</description>
		</variable>
		<variable>
			<ID>PolyComp</ID>
			<method>calculatePolyComp</method>
			<description>Metrics.Variable.Descr.PolyComp</description>
		</variable>
		<variable>
			<ID>BondComp</ID>
			<method>calculateBondComp</method>
			<description>Metrics.Variable.Descr.BondComp</description>
		</variable>
		<variable>
			<ID>RigidComp</ID>
			<method>calculateRigidComp</method>
			<description>Metrics.Variable.Descr.RigidComp</description>
		</variable>
		<variable>
			<ID>Nrigid</ID>
			<method>calculateNrigid</method>
			<description>Metrics.Variable.Descr.Nrigid</description>
		</variable>
		<variable>
			<ID>Depth</ID>
			<method>calculateDepth</method>
			<description>Metrics.Variable.Descr.Depth</description>
		</variable>
		<variable>
			<ID>CM</ID>
			<method>calculateCM</method>
			<description>Metrics.Variable.Descr.CM</description>
		</variable>
	</variables>
	<algorithms>
		<algorithm>
			<ID>D</ID>
			<name>Metrics.Algorithm.Name.D</name>
			<formula>A / (N * (N - 1))</formula>
			<description>Metrics.Algorithm.Descr.D</description>
		</algorithm>
		<algorithm>
			<ID>CNC</ID>
			<formula>(A + 1) / N</formula>
			<description>Metrics.Algorithm.Descr.CNC</description>
		</algorithm>
		<algorithm>
			<ID>DeN</ID>
			<formula>A / N</formula>
			<description>Metrics.Algorithm.Descr.DeN</description>
		</algorithm>
		<algorithm>
			<ID>DeT</ID>
			<formula>A / T</formula>
			<description>Metrics.Algorithm.Descr.DeT</description>
		</algorithm>
		<algorithm>
			<ID>DeP</ID>
			<formula>A / P</formula>
			<description>Metrics.Algorithm.Descr.DeP</description>
		</algorithm>
			<algorithm>
			<ID>Sep</ID>
			<formula>Cut / N</formula>
			<description>Metrics.Algorithm.Descr.Sep</description>
		</algorithm>
		<algorithm>
			<ID>Seq</ID>
			<formula>SeqN / N</formula>
			<description>Metrics.Algorithm.Descr.Seq</description>
		</algorithm>
		<algorithm>
			<ID>RR</ID>
			<formula>RN / N</formula>
			<description>Metrics.Algorithm.Descr.RR</description>
		</algorithm>
		<algorithm>
			<ID>CH</ID>
			<name>Metrics.Algorithm.Name.CH</name>
			<formula>-(RT / T * log2( RT / T ) + RP / P * log2( RP / P))</formula>
			<description>Metrics.Algorithm.Descr.CH</description>
		</algorithm>
		<algorithm>
			<ID>Cyc</ID>
			<formula>CycN / N</formula>
			<description>Metrics.Algorithm.Descr.Cyc</description>
			<thresholds>
				<threshold>
					<range>
						<high>0.2</high>
					</range>
					<state>green</state>
				</threshold>
				<threshold>
					<range>
						<low>0.2</low>
						<high>0.5</high>
					</range>
					<state>yellow</state>
				</threshold>
				<threshold>
					<range>
						<low>0.5</low>
					</range>
					<state>red</state>
				</threshold>
			</thresholds>
		</algorithm>
		<algorithm>
			<ID>CS</ID>
			<formula>N / TreeComp</formula>
			<description>Metrics.Algorithm.Descr.CS</description>
		</algorithm>
		<algorithm>
			<ID>N_TreeCompMax</ID>
			<formula>max( max( TrivialComp , PolyComp ) , max( BondComp , RigidComp ) )</formula>
			<description>Metrics.Algorithm.Descr.N_TreeCompMax</description>
		</algorithm>
		<algorithm>
			<ID>Str</ID>
			<formula>Nrigid / N</formula>
			<description>Metrics.Algorithm.Descr.Str</description>
		</algorithm>
		<algorithm>
			<ID>Test1</ID>
			<formula>Test2 * N</formula>
			<description>Metrics.Algorithm.Descr.Cyc</description>
		</algorithm>
		<algorithm>
			<ID>Test2</ID>
			<formula>Test3 * A</formula>
			<description>Metrics.Algorithm.Descr.Cyc</description>
		</algorithm>
		<algorithm>
			<ID>Test3</ID>
			<formula>Test1 * P</formula>
			<description>Metrics.Algorithm.Descr.Cyc</description>
		</algorithm>
		<algorithm>
			<ID>Test4</ID>
			<formula>YPC</formula>
			<description>Metrics.Algorithm.Descr.Cyc</description>
		</algorithm>
	</algorithms>
</configuration>