<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>
  	<skipIfNotSupported/>
  	<programGroup defaultName="WoPeD" location="applications"/>
  	<shortcut
    	name="WoPeD"
    	target="$INSTALL_PATH/WoPeD"
    	commandLine=""
    	workingDirectory="$INSTALL_PATH"
    	description="Start WoPeD - The Workflow Petrinet Designer"
    	iconFile="$INSTALL_PATH/woped-icon16.png"
    	iconIndex="0"
    	initialState="normal"
    	programGroup="yes"
    	desktop="yes"
    	applications="no"
    	startMenu="no"
    	startup="no"
    	type="Application"
		encoding="UTF-8"
		terminal="false"
    	url="$INSTALL_PATH/WoPeD">
		   	<createForPack name="Executable"/>
  	</shortcut>
  	<shortcut
    	name="WoPeD Manual"
    	target="$INSTALL_PATH/doc/html/index.htm"
    	commandLine=""
    	workingDirectory="$INSTALL_PATH/doc/html"
    	description="Browse the WoPeD User Manual"
    	iconFile=""
    	iconIndex="0"
    	initialState="normal"
    	programGroup="yes"
    	desktop="yes"
    	applications="no"
    	startMenu="no"
    	startup="no"
    	url="$INSTALL_PATH/doc/html/index.htm"
		type="Link"
		encoding="UTF-8">
    		<createForPack name="HTML Manual"/>
  	</shortcut>
</shortcuts>
