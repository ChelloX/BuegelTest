<?xml version="1.0" encoding="UTF-8"?>
<project name="module_stanford_parser" default="compile.module.stanford_parser">
  <dirname property="module.stanford_parser.basedir" file="${ant.file.module_stanford_parser}"/>
  
  <property name="module.jdk.home.stanford_parser" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.stanford_parser" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.stanford_parser" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.stanford_parser" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="stanford_parser.output.dir" value="${module.stanford_parser.basedir}/../../out/production/Stanford Parser"/>
  <property name="stanford_parser.testoutput.dir" value="${module.stanford_parser.basedir}/../../out/test/Stanford Parser"/>
  
  <path id="stanford_parser.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="stanford_parser.module.production.classpath">
    <path refid="${module.jdk.classpath.stanford_parser}"/>
    <path refid="library.tools.classpath"/>
    <path refid="library.xalan-j-2.7.1.classpath"/>
    <path refid="library.lib4.classpath"/>
    <path refid="library.bin.classpath"/>
    <path refid="library.lib2.classpath"/>
    <path refid="library.stanford-parser-2010-08-16.classpath"/>
    <path refid="library.jaxen-1.1.2.classpath"/>
    <path refid="library.xml-apis-2.0.2.classpath"/>
  </path>
  
  <path id="stanford_parser.runtime.production.module.classpath">
    <pathelement location="${stanford_parser.output.dir}"/>
    <path refid="library.tools.classpath"/>
    <path refid="library.xalan-j-2.7.1.classpath"/>
    <path refid="library.lib4.classpath"/>
    <path refid="library.bin.classpath"/>
    <path refid="library.lib2.classpath"/>
    <path refid="library.stanford-parser-2010-08-16.classpath"/>
    <path refid="library.jaxen-1.1.2.classpath"/>
    <path refid="library.xml-apis-2.0.2.classpath"/>
  </path>
  
  <path id="stanford_parser.module.classpath">
    <path refid="${module.jdk.classpath.stanford_parser}"/>
    <pathelement location="${stanford_parser.output.dir}"/>
    <path refid="library.tools.classpath"/>
    <path refid="library.xalan-j-2.7.1.classpath"/>
    <path refid="library.lib4.classpath"/>
    <path refid="library.bin.classpath"/>
    <path refid="library.lib2.classpath"/>
    <path refid="library.stanford-parser-2010-08-16.classpath"/>
    <path refid="library.jaxen-1.1.2.classpath"/>
    <path refid="library.xml-apis-2.0.2.classpath"/>
  </path>
  
  <path id="stanford_parser.runtime.module.classpath">
    <pathelement location="${stanford_parser.testoutput.dir}"/>
    <pathelement location="${stanford_parser.output.dir}"/>
    <path refid="library.tools.classpath"/>
    <path refid="library.xalan-j-2.7.1.classpath"/>
    <path refid="library.lib4.classpath"/>
    <path refid="library.bin.classpath"/>
    <path refid="library.lib2.classpath"/>
    <path refid="library.stanford-parser-2010-08-16.classpath"/>
    <path refid="library.jaxen-1.1.2.classpath"/>
    <path refid="library.xml-apis-2.0.2.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.stanford_parser">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.stanford_parser">
    <patternset refid="excluded.from.module.stanford_parser"/>
  </patternset>
  
  
  <target name="compile.module.stanford_parser" depends="compile.module.stanford_parser.production,compile.module.stanford_parser.tests" description="Compile module Stanford Parser"/>
  
  <target name="compile.module.stanford_parser.production" depends="register.custom.compilers" description="Compile module Stanford Parser; production classes"/>
  
  <target name="compile.module.stanford_parser.tests" depends="register.custom.compilers,compile.module.stanford_parser.production" description="compile module Stanford Parser; test classes" unless="skip.tests"/>
  
  <target name="clean.module.stanford_parser" description="cleanup module">
    <delete dir="${stanford_parser.output.dir}"/>
    <delete dir="${stanford_parser.testoutput.dir}"/>
  </target>
</project>